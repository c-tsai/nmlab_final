  "source": "pragma solidity ^0.5.0;\n\ncontract SproutApp {\n    event OnSproutAdded(uint sproutId);\n\n    struct Sprout {\n       uint id;\n       bool isPlanted;\n    }\n    \n    Sprout[25] sprouts;\n    function addSprout(uint _id) public {\n        sprouts[_id].id = _id;\n        sprouts[_id].isPlanted = true;\n        \n        emit OnSproutAdded(_id);\n    }\n}",
